<resources>
    <!-- Bootleg unneccesary gradient declaration | Just for our shishu bars -->
    <java-symbol type="color" name="bootleg_accent_gradient_end_color" />

    <!-- Device keyhandlers -->
    <java-symbol type="array" name="config_deviceKeyHandlerLibs" />
    <java-symbol type="array" name="config_deviceKeyHandlerClasses" />

    <!-- For dynamic default font styles -->
    <java-symbol type="string" name="config_bodyFontFamily" />

    <!-- Package Manager -->
    <java-symbol type="array" name="config_deviceDisabledComponents" />
    <java-symbol type="array" name="config_globallyDisabledComponents" />
    <java-symbol type="array" name="config_forceEnabledComponents" />

    <!-- Light / Regular style fonts -->
    <java-symbol type="string" name="config_lightFontFamily" />
    <java-symbol type="string" name="config_regularFontFamily" />

    <!-- App error dialog -->
    <java-symbol type="id" name="aerr_copy" />
    <java-symbol type="string" name="url_copy_success" />
    <java-symbol type="string" name="url_copy_failed" />

    <!-- Camera -->
    <java-symbol type="array" name="config_cameraAuxPackageAllowList" />
    <java-symbol type="array" name="config_cameraAuxPackageExcludeList" />
    <java-symbol type="array" name="config_cameraHFRPrivAppList" />

    <!-- Whether to force enable multi resolution configuration for camera -->
    <java-symbol type="bool" name="config_forceMultiResolution" />

    <!-- Whether devices suports in-display fingerprint when screen is off -->
    <java-symbol type="bool" name="config_supportsScreenOffUdfps" />

    <!-- Whether device supports in-display fingerprint -->
    <java-symbol type="bool" name="config_supportsUdfps" />

    <!-- Should we listen for fingerprints when the screen is off?  Devices
         with a rear-mounted sensor want this, but certain devices have
         the sensor embedded in the power key and listening all the time
         causes a poor experience. -->
    <java-symbol type="bool" name="config_fingerprintWakeAndUnlock" />

    <!-- Power Menu -->
    <java-symbol name="reboot_to_bootloader_title" type="string" />
    <java-symbol name="reboot_to_bootloader_message" type="string" />
    <java-symbol name="reboot_to_recovery_title" type="string" />
    <java-symbol name="reboot_to_recovery_message" type="string" />
    <java-symbol name="reboot_system_title" type="string" />
    <java-symbol name="reboot_system_message" type="string" />
    <java-symbol type="array" name="config_rebootActionsList" />

    <!-- Whether to cancel fingerprint operation if not idle -->
    <java-symbol type="bool" name="config_fpCancelIfNotIdle" />

    <!-- Transparent color variants -->
    <java-symbol type="color" name="system_accent_transparent_background_light" />
    <java-symbol type="color" name="system_accent_transparent_background_dark" />

    <!-- These are system packages exempted from privacy indicator -->
    <java-symbol type="array" name="config_indicatorExemptedPackageNames" />

    <!-- PowerOffAlarmService -->
    <java-symbol type="string" name="notification_channel_poweroff_alarm" />
    <java-symbol type="string" name="poweroff_alarm_title" />
    <java-symbol type="string" name="poweroff_alarm_body" />
    <java-symbol type="drawable" name="ic_alarm_on" />

    <!-- App lock -->
    <java-symbol type="string" name="unlock_application" />
    <java-symbol type="array" name="config_appLockAllowedSystemApps" />

    <!-- Define device configs on boot -->
    <java-symbol type="array" name="global_device_configs_override" />
    <java-symbol type="array" name="device_configs_override" />

    <!-- Device hardware keys -->
    <java-symbol type="integer" name="config_deviceHardwareWakeKeys" />  

     <!-- Sensor block -->
     <java-symbol type="array" name="config_blockPackagesSensorDrain" />

    <!-- Whether the device supports Smart Pixels -->
    <java-symbol type="bool" name="config_supportSmartPixels" />
</resources>
